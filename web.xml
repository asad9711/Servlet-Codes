<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Learnserv</display-name>
  <servlet>
    <servlet-name>Servconfig</servlet-name>
    <servlet-class>Servconfig</servlet-class>
    <init-param>
      <param-name>driver</param-name>
      <param-value>com.mysql.jdbc.Driver</param-value>
    </init-param>
    <init-param>
      <param-name>image</param-name>
      <param-value>tweeter1.jpg</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servconfig</servlet-name>
    <url-pattern>/Servconfig</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>asd.html</welcome-file>
  </welcome-file-list>
</web-app>